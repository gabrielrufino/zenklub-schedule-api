    "@types/jest": "^26.0.23",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yamljs": "^0.2.31",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "nodemon": "^2.0.9",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "ts-standard": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.4",
    "wait-on": "^6.0.0"
  }
